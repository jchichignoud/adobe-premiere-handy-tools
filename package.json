{
  "name": "premiere-handy-tools",
  "version": "2.0.0",
  "cep": {
    "name": "Premiere Handy Tools",
    "id": "com.jchichignoud.handytools",
    "hosts": "PPRO@[12.0,13.9]"
  },
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"yarn run start-js\" \"yarn run start-jsx\"",
    "start-js": "parcel src/js/index.html",
    "start-jsx": "parcel watch src/jsx/index.jsx.ts --no-hmr --no-source-maps",
    "build": "yarn run build-js && yarn run build-jsx",
    "build-js": "parcel build src/js/index.html --public-url .",
    "build-jsx": "parcel build src/jsx/index.jsx.ts",
    "zxp": "create-zxp ./dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "draft-js": "^0.10.5",
    "fs-extra": "^6.0.1",
    "lodash": "^4.17.11",
    "pino": "^4.16.1",
    "regexpu-core": "^4.4.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/node-sass": "^3.10.32",
    "@types/pino": "^4.16.1",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "autoprefixer": "^9.4.4",
    "babel-core": "^6.26.3",
    "babel-preset-cep": "^0.0.3",
    "babel-preset-extendscript": "^0.1.1",
    "babel-preset-react": "^6.24.1",
    "cep-interface": "^0.4.2",
    "concurrently": "^3.6.1",
    "create-zxp": "^0.1.0",
    "install": "^0.12.2",
    "node-sass": "^4.11.0",
    "npm": "^6.5.0",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-cep": "1.1.0",
    "postcss-modules": "^1.4.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "types-for-adobe": "pravdomil/types-for-adobe",
    "typescript": "^2.9.2"
  }
}
